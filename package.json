{
  "name": "handy",
  "version": "0.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd src-python && uv pip install -e . &&  cd .. && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-ui": "vite build",
    "build-only": "vite build && run-p build-python",
    "build-python": "cd src-python && uv build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "tauri": "tauri",
    "pack": "run-p build-only && powershell -File pack.ps1",
    "pack-only": "powershell -File pack.ps1"
  },
